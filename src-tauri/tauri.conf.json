{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AzGate",
  "version": "0.1.0",
  "identifier": "com.azgate.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AzGate",
        "width": 1024,
        "height": 576,
        "resizable":false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/classic.ico"
    ]
  }
}
